{
  "name": "cqrs-inventory-system",
  "version": "1.0.0",
  "description": "Example App for CQRS Inventory System",
  "main": "dist/main.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc"
  },
  "nodemonConfig": {
    "watch": [
      "src",
      "lib"
    ],
    "exec": "tsc && node ./dist/src/main.js",
    "ext": "ts,js,json"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.5",
    "nodemon": "^3.1.6",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "rxjs": "^7.8.1"
  },
  "keywords": [
    "cqrs",
    "express",
    "example",
    "template",
    "inventory"
  ],
  "author": "QUAK Kacper Walczak",
  "license": "MIT"
}
